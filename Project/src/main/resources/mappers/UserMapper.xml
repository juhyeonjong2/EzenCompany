<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ezen.ezencompany.mapper.userMapper">
	
	<select id="selectLogin" parameterType="String" resultType="java.util.Map">
		SELECT userid as username, name, password, enabled, authority from user WHERE userid = #{value} 
	</select>

</mapper>